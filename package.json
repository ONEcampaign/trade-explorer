    "@one-data/observable-themes": "^0.6.5",
    "d3": "^7.9.0",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.1.0",
    "htl": "^0.3.1",
    "prettier": "^3.4.2"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18",
    "poetry": ">2.0"
  }
